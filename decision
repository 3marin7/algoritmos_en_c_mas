#include "iostream"

using namespace std;

int main()
{
    int resultado = 0;
    cout << "Cuanto es 39+50? ";
    cin >> resultado;
    if(resultado == 39+50)
    {
        cout << "Respuesta Correcta. Felicitaciones!\n";
    }

    system("PAUSE");
}
